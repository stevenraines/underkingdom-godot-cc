config_version=5

[application]

config/name="Underkingdom Roguelike"
config/description="Turn-based roguelike survival game with procedural generation"
config/version="0.1.0"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

